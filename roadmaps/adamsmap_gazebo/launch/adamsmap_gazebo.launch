<launch>
  <env name="GAZEBO_RESOURCE_PATH" value="$(find adamsmap_gazebo)/gazebo"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="debug" value="false" />
    <arg name="gui" value="true" />
    <arg name="headless" value="false"/>
    <arg name="paused" value="false"/>
    <!-- <arg name="physics" value="none"/> -->
    <arg name="use_sim_time" value="true"/>
    <arg name="verbose" value="false"/>
    <arg name="world_name" value="$(find adamsmap_gazebo)/gazebo/map.world"/>
  </include>

  <!-- <node name="publish_robot_pose" type="publish_robot_pose.py" pkg="adamsmap_gazebo"
    args="$(find adamsmap_gazebo)/data/z_200_4096n_agents10_paths_typepaths_ev_n_trial0.csv"/> -->

  <param name="robot_description"
	 command="$(find xacro)/xacro.py $(find adamsmap_gazebo)/gazebo/discbot.xacro" />
</launch>
