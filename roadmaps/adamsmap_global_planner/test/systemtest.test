<launch>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <param name="base_global_planner" value="adamsmap_global_planner/AdamsmapGlobalPlanner" />
    </node>

    <node pkg="adamsmap_global_planner" type="publish_graph.py" name="publish_graph" output="screen" />
    <node name="tf_base_link_map" pkg="tf" type="static_transform_publisher" args=".1 .1 0 0 0 0 1 map base_link 100" />
</launch>
